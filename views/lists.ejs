<%- include( "header" ) -%>

  <div class="box" id="heading">
    <h1><%= listTitle %></h1>
  </div>
  <div class="box">
    <% for ( const item of itemList ){ %>
      <div class="item">
        <input type="checkbox">
        <p><%= item %></p>
      </div>
    <% } %>

    <form class="item" action="/" method="post">
      <input type="text" name="newItem" placeholder="Nueva tarea" autocomplete="off">
      <button type="submit" name="button">+</button>
    </form>
  </div>

<%- include( "footer" ) -%>
